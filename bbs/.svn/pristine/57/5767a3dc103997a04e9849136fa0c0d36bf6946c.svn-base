<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.yhcrt.weihu.bbs.entity">
	<class name="BbsFriendShip" table="jb_friendship">
		<meta attribute="sync-DAO">false</meta>
		<cache usage="read-write"/>
		<id name="id" type="integer" column="friendship_id"><generator class="identity"/></id>
		<property name="status" column="status" type="integer" not-null="true"></property>
		<property name="createTime" column="create_time" type="timestamp" not-null="true"></property>
		<property name="applyDesc" column="apply_desc" type="string" not-null="false" length="32"></property>
		<property name="managerDesc" column="manager_desc" type="string" not-null="false" length="32"></property>
		<many-to-one name="user" column="user_id" class="BbsUser" not-null="true"></many-to-one>
		<many-to-one name="friend" column="friend_id" class="BbsUser" not-null="true"></many-to-one>
	</class>
</hibernate-mapping>