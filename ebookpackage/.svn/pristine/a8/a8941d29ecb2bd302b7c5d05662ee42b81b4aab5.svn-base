package com.whty.ebp.manage.model;

import java.util.Date;

public class Message {
	/* 逻辑主键ID */
	private String Id;
	/* 消息类型ID */
	private String messageTypeId;
	/* 消息类型名称 */
	private String messageTypeName;
	/* 主题 */
	private String theme;
	/* 内容,根据消息类型判断是以html文件形式存在服务器还是一段文字 */
	private String content;
	/* 发件人ID */
	private String senderId;
	/* 发件人名称 */
	private String senderName;
	/* 发件机构ID */
	private String sendOrgId;
	/* 发件机构名称 */
	private String sendOrgName;
	/* 接收对象,0--产品、1--个人 */
	private String receiveObjType;
	/* 接收对象ID,多个用逗号分隔 */
	private String receiveObjId;
	/*接收对象姓名,多个用逗号分隔  */
	private String receiveObjName;
	/* 创建时间 */
	private Date createTime;
	/* 发布时间 */
	private Date releaseTime;
	/* 有效状态,0--有效、1--无效 */
	private String status;
	/* 消息的读取状态,0--已读、1--未读、2--删除 */
	private String messageStateStatus;
	public String getId() {
		return Id;
	}
	public void setId(String id) {
		Id = id;
	}
	public String getMessageTypeId() {
		return messageTypeId;
	}
	public void setMessageTypeId(String messageTypeId) {
		this.messageTypeId = messageTypeId;
	}
	public String getMessageTypeName() {
		return messageTypeName;
	}
	public void setMessageTypeName(String messageTypeName) {
		this.messageTypeName = messageTypeName;
	}
	public String getTheme() {
		return theme;
	}
	public void setTheme(String theme) {
		this.theme = theme;
	}
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	public String getSenderId() {
		return senderId;
	}
	public void setSenderId(String senderId) {
		this.senderId = senderId;
	}
	public String getSenderName() {
		return senderName;
	}
	public void setSenderName(String senderName) {
		this.senderName = senderName;
	}
	public String getSendOrgId() {
		return sendOrgId;
	}
	public void setSendOrgId(String sendOrgId) {
		this.sendOrgId = sendOrgId;
	}
	public String getSendOrgName() {
		return sendOrgName;
	}
	public void setSendOrgName(String sendOrgName) {
		this.sendOrgName = sendOrgName;
	}
	public String getReceiveObjType() {
		return receiveObjType;
	}
	public void setReceiveObjType(String receiveObjType) {
		this.receiveObjType = receiveObjType;
	}
	public String getReceiveObjId() {
		return receiveObjId;
	}
	public void setReceiveObjId(String receiveObjId) {
		this.receiveObjId = receiveObjId;
	}
	public String getReceiveObjName() {
		return receiveObjName;
	}
	public void setReceiveObjName(String receiveObjName) {
		this.receiveObjName = receiveObjName;
	}
	public Date getCreateTime() {
		return createTime;
	}
	public void setCreateTime(Date createTime) {
		this.createTime = createTime;
	}
	public Date getReleaseTime() {
		return releaseTime;
	}
	public void setReleaseTime(Date releaseTime) {
		this.releaseTime = releaseTime;
	}
	public String getStatus() {
		return status;
	}
	public void setStatus(String status) {
		this.status = status;
	}
	public String getMessageStateStatus() {
		return messageStateStatus;
	}
	public void setMessageStateStatus(String messageStateStatus) {
		this.messageStateStatus = messageStateStatus;
	}
}
