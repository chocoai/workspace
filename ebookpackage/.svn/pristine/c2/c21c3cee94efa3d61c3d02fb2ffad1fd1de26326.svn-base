/*deset start*/
html,body,h1,h2,h3,h4,h5,h6,div,p,span,section,article,ul,li,input,textarea,button{margin:0; padding:0; border:0 none; outline:0 none;}
body{font-size:14px; font-family: "microsoft yahei"; line-height:1.6em;}
h1,h2,h3,h4,h5,h6{font-size: 100%; font-weight: normal;}
ul,li{list-style: none;}
input,textarea,button{appearance:none; -webkit-appearance:none; font-family: "microsoft yahei"; font-size:100%; border-radius: 0px;}
input:disabled{background:none; -webkit-tap-highlight-color: rgba(255,255,255,0);}
a:link{text-decoration: none;}
a:visited{color:inherit;}
a:hover{color:inherit; text-decoration: none;}
a:active{color:inherit;}

/*reset end*/
.download_main{ position: relative;}
.download_main .bg img{ width: 100%; vertical-align: middle;}
.download_main a.down_btn{ position: absolute; top:28%; left: 15%; width: 70%; display: block; text-align: center; border-radius: 25px;height: 50px; font-size: 24px; line-height: 50px; color: #fff; font-family: "黑体"; background: #4ba5fd; background: -ms-linear-gradient(top, #54cde8,  #4ba5fd); background: #4ba5fd; background: -webkit-linear-gradient(top, #54cde8,  #4ba5fd);  background: -o-linear-gradient(top, #54cde8,  #4ba5fd); 
background: -moz-linear-gradient(top, #54cde8,  #4ba5fd);  box-shadow: 0 0 10px #9aeafd}