<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.whty.ebp.manage.dao.PkgInfoDao">
	<resultMap id="pkgMap" type="com.whty.ebp.manage.model.PkgInfo">
		<id column="id" property="id"></id>
		<result column="pkg" property="pkg"></result>
	</resultMap>
	
	<select id="selectPkgInfo" resultMap="pkgMap">
		select * from ta_pkg_info where 1=1
	</select>
</mapper>