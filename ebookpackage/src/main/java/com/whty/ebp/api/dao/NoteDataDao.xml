		<result column="id" property="id" />
		<result column="userId" property="user_Id" />
		<result column="userName" property="user_name" />
		<result column="orgId" property="org_Id" />
		<result column="orgName" property="org_Name" />
		<result column="chapterId" property="chapter_id" />
