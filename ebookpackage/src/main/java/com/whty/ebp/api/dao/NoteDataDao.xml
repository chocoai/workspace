	<insert id="save" parameterType="com.whty.ebp.api.model.NoteData">
		insert into EBP_NOTE_DATA (
		id
		<if test="userId != null">,user_id</if>
		<if test="userName != null">,user_name</if>
		<if test="platformCode != null">,platform_code</if>
		<if test="loginPlatformCode != null">,login_platform_code</if>
		<if test="classId != null">,class_id</if>
		<if test="className != null">,class_name</if>
		<if test="teacherId != null">,teacher_id</if>
		<if test="teacherName != null">,teacher_name</if>
		<if test="subjectId != null">,subject_id</if>
		<if test="subjectName != null">,subject_name</if>
		<if test="chapterId != null">,chapter_id</if>
		<if test="chapterName != null">,chapter_name</if>
		<if test="orgId != null">,org_id</if>
		<if test="orgName != null">,org_name</if>
		<if test="dataFileUrl != null">,data_file_url</if>
		<if test="interactiveId != null">,interactive_Id</if>
		) values(
		#{id}
		<if test="userId != null">,#{userId}</if>
		<if test="userName != null">,#{userName}</if>
		<if test="platformCode != null">,#{platformCode}</if>
		<if test="loginPlatformCode != null">,#{loginPlatformCode}</if>
		<if test="classId != null">,#{classId}</if>
		<if test="className != null">,#{className}</if>
		<if test="teacherId != null">,#{teacherId}</if>
		<if test="teacherName != null">,#{teacherName}</if>
		<if test="subjectId != null">,#{subjectId}</if>
		<if test="subjectName != null">,#{subjectName}</if>
		<if test="chapterId != null">,#{chapterId}</if>
		<if test="chapterName != null">,${chapterName}</if>
		<if test="orgId != null">,#{orgId}</if>
		<if test="orgName != null">,#{orgName}</if>
		<if test="dataFileUrl != null">,#{dataFileUrl}</if>
		<if test="interactiveId != null">,#{interactiveId}</if>
		)
	</insert>

	
