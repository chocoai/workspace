		<if test="platformCode != null">,#{platformCode}</if>
		<if test="loginPlatformCode != null">,#{loginPlatformCode}</if>
		<if test="orgId != null">,#{orgId}</if>
		<if test="orgName != null">,#{orgName}</if>
		<if test="dataFileUrl != null">,#{dataFileUrl}</if>
