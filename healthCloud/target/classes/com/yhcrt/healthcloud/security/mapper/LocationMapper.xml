		select
		<include refid="Base_Column_List" />
		from location b ,(SELECT a.imei imei,MAX(a.location_time)
		location_time from location a GROUP BY a.imei) a
		WHERE b.imei=a.imei
		AND b.location_time=a.location_time and a.imei LIKE
		#{imei}
	</select>
