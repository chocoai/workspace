	<select id="getLocation" parameterType="map" resultMap="BaseResultMap">
		select b.location_id, b.imei, b.longitude, b.latitude, b.addr,
		b.location_time from (
		select imei,max(location_time) location_time
		from location where imei in
		<foreach collection="imeiList" item="imei" open="(" separator="," close=")">
			#{imei}
		</foreach>
		group by imei) a
		left join location b ON a.imei = b.imei and
		a.location_time=b.location_time
		left join device c on a.imei = c.imei
		where c.`status` = 0 and c.org_id IN
		<foreach collection="list" item="orgId" open="(" separator="," close=")">
			#{orgId}
		</foreach>
	</select>

	<select id="getOrbit" parameterType="java.lang.String"
		resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from location
		where imei=#{imei,jdbcType=VARCHAR}
		and
		DATE_FORMAT(location_time,'%Y-%m-%d')=DATE_FORMAT(NOW(),#{locationTime,jdbcType=VARCHAR})
		order by location_time
	</select>

	<select id="selectByPrimaryKey" parameterType="java.lang.String"
		resultMap="BaseResultMap">
