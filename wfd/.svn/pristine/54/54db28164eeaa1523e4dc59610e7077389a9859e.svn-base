  <select id="selectUserByPlate" parameterType="map" resultMap="BaseResultMap">
  	select * from t_user where school_id=#{schoolId} and id in
(select distinct(user_id) from t_class_user t1 right join t_class_plate t2 on t1.class_id=t2.class_id
where 1=1
    <if test="plateId !=null and plateId !=''">
      and plate_id=#{plateId}
    </if>
union all select distinct(user_id) from t_plate_user
where 1=1
    <if test="plateId !=null and plateId !=''">
      and plate_id=#{plateId}
    </if>
) order by convert(name using gbk)
  </select>
  
  <select id="selectByPlate" parameterType="map" resultMap="BaseResultMap">
  	select * from t_user where id in(select user_id from t_plate_user where plate_id=#{plateId}) order by convert(name using gbk)
  </select>
  
  <select id="selectByClass"  parameterType="map" resultMap="BaseResultMap">
  	select t2.* from t_class_user class_user left join t_user t2 on class_user.user_id = t2.id 
