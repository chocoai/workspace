/*20170612奖牌榜数据中心*/
.medal-bg {
	width: 100%;
	background: #fff;
	padding-bottom: 50px;
}

.header {
    background: #222;
    background-repeat: no-repeat;
    background-position: center 0;
    -webkit-background-size: cover;
    background-size: cover;
    padding: 208px 0 0;
    text-align: center;
    min-height: 400px;
    position: relative;
}

.logoimg {
    position: absolute;
    left: 1%;
    right: 1%;
    bottom: -26px;
}

.logotxt {
    margin-bottom: 30px;
    border: 0;
}

.logotxt h1 {
	color: #fff;
	text-shadow: 0 0 3px rgba(255,255,255,0.2);
}

.medal-main {
    margin-top: 90px;
}

.gray-medal-bg {
	background: #f8f8f8;
}

.table-medal {
	border-top: 2px solid #337ab7;
}

.table-medal tr td,.table-medal tr th {
    text-align: center;
}

.panel-medal {
	padding-bottom: 30px;
	border-color: #ddd;
}
.add {
	line-height:34px;
	color:#337ab7;
	cursor:pointer;
	transition:all 0.5s;
	
}

.minus {
	line-height:34px;
	color:#337ab7;
	cursor:pointer;
	transition:all 0.5s;
}

.minus:hover {
	transform: scale(1.5);
}

.add:hover {
	transform: scale(1.5);
}
/***节点树搜索***/
.bumen-search-box {
	border: 1px solid #ccc;
    border-radius: 4px;
}
.bumen-nobor {
	border: none;
}
.bumen-addon {
	padding: 0; border: none; 
	background: #fff;
}
.bumen-bili {
	height: 34px; 
	display: block; 
	margin-bottom: 0;
}
.bumen-bili-inner {
	height: 34px; 
	line-height: 34px; 
	margin-bottom: 0;
	font-size: 13px;
	color: #aaa;
}
.bumen-updown {
	height: 34px; 
	width: 20px; 
	position: relative;
	padding: 0 3px;
}
.bumen-up {
	height: 17px; 
	width: 20px; 
	line-height: 22px; 
	position: absolute; 
	top: 0; 
	left: 0;
}
.bumen-down {
	height: 17px; 
	width: 20px; 
	line-height: 12px; 
	position: absolute; 
	top: 17px; 
	left: 0;
}
.bumen-updown a {
	text-decoration: none;
	cursor: pointer;
}
