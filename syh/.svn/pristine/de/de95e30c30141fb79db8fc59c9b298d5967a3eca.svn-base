package com.yhcrt.entity.system;

public class SysDeptUser  implements java.io.Serializable{
    private Integer userId;

    private Integer deptCid;
    
    public SysDeptUser(Integer userId,Integer deptCid){
    	this.userId = userId;
    	this.deptCid = deptCid;
    }

    public Integer getUserId() {
        return userId;
    }

    public void setUserId(Integer userId) {
        this.userId = userId;
    }

    public Integer getDeptCid() {
        return deptCid;
    }

    public void setDeptCid(Integer deptCid) {
        this.deptCid = deptCid;
    }
}