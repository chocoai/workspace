<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!-- 模态框（查询成绩） -->
<div class="modal fade" id="viewAthleteScore" tabindex="-1"
	role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal"
					aria-hidden="true">&times;</button>
				<h4 class="modal-title">查看成绩详情</h4>
			</div>
			<div class="modal-body">
				<div class="row">
					<div class="form-horizontal">
						<div class="col-md-11">
							<div class="form-group">
								<label class="col-md-3 control-label">项目名称</label>
								<div class="col-md-8">
									<input type="text" id="viewProjectName"
										class="form-control" readonly> <input type="hidden"
										id="viewProjectCid" >
								</div>
							</div>
						</div>
						<div class="col-md-11">
							<div class="form-group">
								<label class="col-md-3 control-label">参赛人/队名称</label>
								<div class="col-md-8">
									<input type="text" 
										class="form-control" id="viewAthleteName" readonly> <input
										type="hidden"  id="viewParticipatId">
								</div>
							</div>
						</div>
						<div class="col-md-11">
							<div class="form-group">
								<label class="col-md-3 control-label">所属单位</label>
								<div class="col-md-8">
									<input type="text" 
										class="form-control" id="viewUnitName" readonly>
								</div>
							</div>
						</div>
						<div class="col-md-11">
							<div class="form-group">
								<label class="col-md-3 control-label">成绩单位</label>
								<div class="col-md-8">
									<input type="text" id="viewResultTypeVal" class="form-control" readonly>
								</div>
							</div>
						</div>
						<div class="col-md-11">
							<input type="hidden" id="viewResultType" >
								<div class="form-group" id="showScores">
									<label class="col-md-3 control-label">成绩</label>
									<div class="col-md-8">
										<input type="text" class="form-control" id="viewScores"
											placeholder="请输入成绩" readOnly>
									</div>
								</div>
							<div class="form-group">
								<label class="col-md-3 control-label">名次</label>
								<div class="col-md-8">
									<input type="text" class="form-control" id="viewRanking"
										placeholder="请输入名次" readOnly>
								</div>
							</div>
						</div>
						<div class="col-md-11">
							<div class="form-group">
								<label class="col-md-3 control-label">奖牌</label>
								<div class="col-md-3">
									<input type="text" id="viewMedal" class="form-control" readonly>
								</div>
								<div class="col-md-5">
									<input type="text" id="viewMedalNum" class="form-control"
										 readOnly>
								</div>
							</div>
							
						</div>
						<div class="col-md-11">
							<div class="form-group">
								<label class="col-md-3 control-label">积分</label>
								<div class="col-md-8">
									<input type="text" id="viewIntrgral" class="form-control" readonly>
								</div>
							</div>
						</div>
						<div class="col-md-11">
							<div class="form-group">
								<label class="col-md-3 control-label">备注</label>
								<div class="col-md-8">
									<textarea class="form-control" rows="3" id="viewBackup" readOnly></textarea>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
		<!-- /.modal-content -->
	</div>
	<!-- /.modal -->
</div>