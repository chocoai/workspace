<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.whty.assis.manage.dao.HdCodeListenDao">
	<resultMap type="com.whty.assis.manage.model.HdCodeListen" id="BaseResultMap">
		<result column="id" property="id"/>
		<result column="code_id" property="codeId"/>
		<result column="licence_code" property="licenceCode"/>
	</resultMap>

	<select id="getList" parameterType="java.lang.String" resultMap="BaseResultMap">
		SELECT * from hd_code_listen_code WHERE licence_code = #{ListenCode} order by id
	</select>

	<insert id="insert">
		insert into hd_code_listen_code (code_id, licence_code)
		values
		<foreach collection="codeIds" item="codeId" index="index" separator=",">
			( #{codeId,jdbcType=VARCHAR}, #{licenceCode,jdbcType=VARCHAR})
		</foreach>
	</insert>

	<delete id="delete" parameterType="java.lang.String">
		delete from hd_code_listen_code WHERE  licence_code = #{licenceCode}
	</delete>

</mapper>