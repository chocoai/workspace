<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.whty.assis.manage.dao.HdCodeDao">
	<resultMap type="com.whty.assis.manage.model.HdCode" id="BaseResultMap">
		<result column="id" property="id"/>
		<result column="pid" property="pid"/>
		<result column="name" property="name"/>
	</resultMap>

	<select id="getList" resultMap="BaseResultMap">
		SELECT * from hd_code WHERE id != 1 order by id
	</select>

	<select id="getListTree" resultMap="BaseResultMap">
		SELECT * from hd_code order by id
	</select>

</mapper>