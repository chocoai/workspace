    <!-- 固定底部的去支付 开始 -->
    <nav class="navbar navbar-default navbar-fixed-bottom mall-parts">
      <div class="container-fluid nopad-lr">
        <div th:if="${type eq 'goods'}" class="mall-fixed-bottom" th:switch="${order.orderStatus}">
        	 <p th:case="0">
		          <a href="javascript:void(0)" class="order-deal ordersPay" th:id="${order.cext1}" lang="goods">立即付款</a>
		          <a href="javaScript:void(0)" class="order-deal orderDel" th:id="${order.orderId}">取消订单</a>
			 </p>
        	 <p th:case="2">
		          <a th:if="${a} ==null" th:href="@{'/h5/admin/myOrdersPeceipt/goods?cid='+${order.orderId}}" class="order-deal order-deal-green">收货</a>
		          <a th:if="${a} !=null" th:href="@{'/h5/admin/myOrdersPeceipt/goods?cid='+${order.orderId}+'&a=1&u='+${u}}" class="order-deal order-deal-green">收货</a>
			 </p>
        	 <p th:case="3">
		          <a th:if="${a} ==null" th:href="@{'/h5/admin/myOrdersComTo/goods?cid='+${order.orderId}}" class="order-deal order-deal-green">评价</a>
		          <a th:if="${a} !=null" th:href="@{'/h5/admin/myOrdersComTo/goods?cid='+${order.orderId}+'&a=1&u='+${u}}" class="order-deal order-deal-green">评价</a>
			 </p>
        	 <p th:case="*">
			 </p>
        </div>
        <div th:if="${type eq 'service'}" class="mall-fixed-bottom" th:switch="${order.orderStatus}">
        	 <p th:case="0">
				  <a href="javascript:void(0)" class="order-deal ordersPay" th:id="${order.cext1}" lang="service">立即付款</a>
		          <a href="javaScript:void(0)" class="order-deal orderDel" th:id="${order.orderId}">取消订单</a>
			 </p>
        	 <p th:case="3">
		          <a th:if="${a} ==null" th:href="@{'/h5/admin/myOrdersComplete/service?cid='+${order.orderId}}" class="order-deal order-deal-green">完成</a>
		          <a th:if="${a} !=null" th:href="@{'/h5/admin/myOrdersComplete/service?cid='+${order.orderId}+'&a=1&u='+${u}}" class="order-deal order-deal-green">完成</a>
			 </p>
        	 <p th:case="4">
