      <script type="text/javascript">
  	$(function(){
  		var versionid = GetQueryString("version_id");
     	$.ajax({
	    	type: "get",
            url: '/api/services?pdata={"sid":"healthCloud.app.getVersionDesc","biz":{"version_id":"'+versionid+'"}}',
            dataType: "json",
            success: function(data){
            	var biz = data.biz;
            	var cUpdateDesc = biz.cUpdateDesc;
            	$("#cUpdateDesc").text(cUpdateDesc);
           	}
        }); 
	})
	
	function GetQueryString(name)
	{
	     var reg = new RegExp("(^|&)"+ name +"=([^&]*)(&|$)");
	     var r = window.location.search.substr(1).match(reg);
	     if(r!=null)return  unescape(r[2]); return null;
	}
	</script>
