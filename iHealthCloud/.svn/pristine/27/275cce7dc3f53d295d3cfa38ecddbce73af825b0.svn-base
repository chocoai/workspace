	<select id="getLatestSleepData" parameterType="java.lang.String" resultMap="SleepResultMap">
		SELECT sleep_duration, deep_sleep_duration, sleep_quality, data_date, upload_time FROM hd_sleep
 		WHERE member_id = #{memberId,jdbcType=VARCHAR} ORDER BY upload_time DESC LIMIT 1
	</select>
	
	<select id="listSleepDataByMemberId" parameterType="java.lang.String" resultMap="SleepResultMap">
		SELECT sleep_duration, deep_sleep_duration, sleep_quality, data_date, upload_time FROM hd_sleep
 		WHERE member_id = #{memberId,jdbcType=VARCHAR} ORDER BY upload_time DESC 
	</select>
	
	<select id="getSleepDataForWeek" parameterType="java.lang.String" resultMap="SleepResultMap">
		SELECT sleep_duration, deep_sleep_duration, sleep_quality, DATE_FORMAT(data_date, '%m-%d') AS data_date,upload_time
		  FROM hd_sleep WHERE member_id = #{memberId,jdbcType=VARCHAR}
		ORDER BY DATE_FORMAT(data_date, '%Y-%m-%d') ASC LIMIT 7;
	</select>
	
	<select id="getMaxMinAvgSleepDataForWeek" parameterType="java.lang.String" resultType="com.yhcrt.iHealthCloud.pojo.DataItem">
		SELECT max(t.deep_sleep_duration) as max, min(t.deep_sleep_duration) as min, round(avg(t.deep_sleep_duration)) as avg FROM(
		  SELECT deep_sleep_duration, DATE_FORMAT(data_date, '%m-%d') AS data_date
		    FROM hd_sleep WHERE member_id = #{memberId,jdbcType=VARCHAR}
		  ORDER BY DATE_FORMAT(data_date, '%Y-%m-%d') DESC LIMIT 7) t
	</select>
	
	<select id="listSleepDataByTime" parameterType="map" resultMap="SleepResultMap">
		SELECT sleep_duration, deep_sleep_duration, sleep_quality, substring(hd_sleep.data_date,6,5) as data_date, upload_time FROM hd_sleep
 		WHERE member_id = #{memberId,jdbcType=VARCHAR} 
			AND hd_sleep.data_date &gt;= #{startTime,jdbcType=VARCHAR}
			AND hd_sleep.data_date &lt;= #{endTime,jdbcType=VARCHAR} 
			ORDER BY upload_time DESC
	</select>
	
