	<!-- 获取最近七天的数据 -->
	<select id="selectHdSleep" parameterType="map" resultMap="BaseResultMap">
		select cid, imei, member_id, SUM(deep_sleep_duration) deep_sleep_duration,
		sleep_quality, data_date, DATE_FORMAT(MAX(upload_time), '%m-%d') upload_time
		from hd_sleep where member_id=#{memberId} 
		and DATE_FORMAT(upload_time, '%Y-%m-%d') &gt;= DATE_FORMAT(#{startTime}, '%Y-%m-%d') 
		and DATE_FORMAT(upload_time, '%Y-%m-%d') &lt;= DATE_FORMAT(#{endTime}, '%Y-%m-%d')
		GROUP BY DATE_FORMAT(upload_time, '%Y-%m-%d') 
		ORDER BY upload_time desc
	</select>
	
