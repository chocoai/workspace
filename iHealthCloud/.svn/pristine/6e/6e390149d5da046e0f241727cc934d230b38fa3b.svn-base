  <select id="ListOnlineDeviceLocation" parameterType="java.lang.String" resultType="com.yhcrt.iHealthCloud.pojo.LocationData">
	  SELECT CASE
		          WHEN length(member.real_name) > 0 THEN member.real_name
		          WHEN length(member.nick_name) > 0 THEN member.nick_name
		          ELSE '-'
		       END AS name,
		       member.member_id as memberId, location.imei as imei, longitude as longitude, latitude as latitude , 
		       addr as addr, max(location_time) as locationTime
		  FROM member, member_device, location
		 WHERE member.member_id = member_device.member_id
		       AND member_device.org_id = #{orgId,jdbcType=VARCHAR}
		       AND location.imei = member_device.imei
		       AND location.location_time > curdate()
		GROUP BY location.imei;
  </select>
  
  <select id="searchOnlineDeviceLocation" parameterType="map" resultType="com.yhcrt.iHealthCloud.pojo.LocationData">
  	<bind name="param" value="'%'+ param2 + '%'" />
	  SELECT CASE
		          WHEN length(member.real_name) > 0 THEN member.real_name
		          WHEN length(member.nick_name) > 0 THEN member.nick_name
		          ELSE '-'
		       END AS name,
		       member.member_id as memberId, location.imei as imei, longitude as longitude, latitude as latitude , 
		       addr as addr, max(location_time) as locationTime
		  FROM member, member_device, location
		 WHERE member.member_id = member_device.member_id
		       AND member_device.org_id = #{orgId,jdbcType=VARCHAR}
		       AND (member_device.imei like #{param} or member.real_name like #{param} or member.nick_name like #{param})
		       AND location.imei = member_device.imei
		       AND location.location_time > curdate()
		GROUP BY location.imei;
  </select>  
  
