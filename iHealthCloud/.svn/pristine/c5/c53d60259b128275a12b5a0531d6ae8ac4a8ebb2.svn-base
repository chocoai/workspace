	<select id="selectBy" resultMap="BaseResultMap">
		select s.*
		from service s ,
		(SELECT org_id id from organization o
		<choose>
			<when test=" service !=null and service.org!=null and service.org.areaId!=null ">
				,(SELECT area.* from area where
				FIND_IN_SET(area.area_id,getChildrenOrg(#{service.org.areaId,jdbcType=VARCHAR}))
				) a WHERE o.area_id = a.area_id
			</when>
			<otherwise>
				WHERE 1=1
			</otherwise>
		</choose>
		And o.status = 0
		<if test=" service !=null and service.org!=null and service.org.orgType != null ">
			AND o.org_type =#{service.org.orgType,jdbcType=VARCHAR}
		</if>
		) a
		WHERE s.org_id =a.id
		<if test=" service !=null and service.serviceType != null">
			AND s.service_type =#{service.serviceType,jdbcType=VARCHAR}
		</if>
		<if test=" service !=null and service.serviceName != null">
			AND s.service_name like CONCAT('%',#{service.serviceName},'%')
		</if>
