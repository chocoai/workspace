	<select id="getMyFollowingForSmall"
		parameterType="string"
		resultMap="smallResultMap">
		select
		cid, member_id, 
		(SELECT phone_no from member T3 where T3.member_id = T1.member_id ) as tel,
		(SELECT real_name from member T3 where T3.member_id = T1.member_id ) as real_name,
		(SELECT head_pic from member T3 where T3.member_id = T1.member_id ) as head_pic,
		(SELECT imei from member_device T4 where T4.member_id = T1.member_id ) as imei,
		follower_id, relation_desc, create_time,  
		CASE  T1.CID WHEN  (SELECT MIN(T2.CID) FROM  member_relationship T2 WHERE T2.member_id = T1.member_id GROUP BY T2.member_id ) THEN 1 ELSE 0 END AS status
		from member_relationship T1 
		where T1.follower_id = #{memberId,jdbcType=VARCHAR}
	</select>
