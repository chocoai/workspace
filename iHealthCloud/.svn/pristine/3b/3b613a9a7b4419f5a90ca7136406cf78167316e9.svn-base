body { background: #fff; font-family: "微软雅黑",Arial,sans-serif; }
* { padding: 0; margin: 0; }
ul,ol { list-style: none; margin-bottom: 0px; }
a { color: #333; text-decoration: none; }
a:link, a:visited, a:hover, a:active, a:focus { text-decoration: none; outline: none; }

/*首页*/
.index-mall { padding-top: 60px; background: #f3f3f3;}
.navbar-mall { height: 60px; background: #fff; border: none; border-bottom: 0.5px solid #e6e6e6; margin-bottom: 0; line-height: 60px; }
.navbar-brand { font-size: 17px; height: 60px; color: #14b45c !important; padding-top: 0; padding-bottom: 0; line-height: 60px;}
.navbar-brand img { height: 24px; width: auto; display: inline-block;}
.mall-icon-top { margin-right: 15px; line-height: 40px; padding: 10px 0; }
.mall-icon-top li { float: left; }
.mall-icon-top li a { padding: 10px; font-size: 20px; color: #a3a3a3; }
@media (min-width:768px) {
	.navbar-header { width: 100% }
	.mall-icon-top { margin-right: 0; }
}
.swiper-pagination-bullet-active { background: #14b45c; }

.twobigtype { float: left; background: #fff; padding: 14px 0; width: 100%; }
.twobigtype-cell { float: left; padding: 8px 10px 8px 15px; width: 100%;}
.twobigtype-cell img { height: 48px; width: auto; display: inline-block; margin-right: 10px; }
.twobigtype-cell span { display: inline-block; font-size: 14px; }
.type-goods { background: #e6faee; }
.type-service { background: #e8f2fe; }
.pad-r7 { padding-right: 7.5px; }
.pad-l7 { padding-left: 7.5px; }
.pad-r10 { padding-right: 10px; }
@media (max-width:370px) {
	.twobigtype-cell img { height: 40px; margin-right: 5px;}
}
@media (min-width:768px) {
	.twobigtype-cell { padding: 20px 15px 20px 40px;}
	.twobigtype-cell img { margin-right: 20px; height: 60px;}
}

.mallindex-cell { float: left; width: 100%;}
.mallindex-cell-hd { float: left; width: 100%; height: 40px; line-height: 40px; font-size: 14px; padding-left: 10px; }
.mallindex-cell-hd em { float: left; width: 3px; height: 14px; background: #14b45c; margin-right: 8px; margin-top: 13px;}
.mallindex-cell-bd { float: left; width: 100%; background: #fff; }
.goods-list { float: left; width: 100%; background: #fff; }
.goods-list li { padding-right: 0; padding-left: 0; padding-bottom: 1px; border-bottom: 1px solid #e6e6e6; text-align: center; }
.goods-list li a { float: left; width: 100%;}
.goods-list li a img.img-goods { width: 100px; height: 100px; margin: 15px auto; }
.goods-list li a p { width: 100%; height: 20px;text-overflow: clip; overflow: hidden; line-height: 20px; margin-bottom: 0; padding-right: 6px; padding-left: 6px; }
.mallcell-title { font-size: 13px; }
.mallcell-smalltitle { font-size: 11px; color: #aaa; }
.mallcell-price { color: #14b45c; font-size: 10px; margin-bottom: 10px !important; }
@media (max-width: 768px) {
	.goods-list li:nth-child(odd) { border-right: 1px solid #e6e6e6;}
	.goods-list li:nth-child(even) { border-right: none; padding-right: 1px;}
}
@media (min-width:768px) {
	.goods-list li a img.img-goods { margin-top: 30px; margin-bottom: 20px; }
	.goods-list li:nth-child(4n) { border-right: none; padding-right: 1px;}
	.goods-list li:nth-child(4n+2),.goods-list li:nth-child(4n+3),.goods-list li:nth-child(4n+1) { border-right: 1px solid #e6e6e6;}
	.mallcell-price { margin-bottom: 30px !important; }
}

.service-list li a img { padding: 6px;}
.service-list li a p { text-align: left; padding-right: 6px; padding-left: 6px; }
.service-list li a p.mallcell-price { margin-bottom: 6px !important; }

.mall-footer { float: left; width: 100%; background: #fff; margin-top: 10px; padding: 15px 10px; text-align: center; }
.footer-list { line-height: 20px; text-align: center;}
.footer-list li { display: inline;}
.footer-list li a { color: #888; padding-left: 20px; padding-right: 20px; font-size: 12px; border-left: 0.5px solid #ddd;}
.footer-list li:first-child a { border-left: none; }
.pra-footer { float: left; width: 100%; line-height: 30px; font-size: 11px; color: #aaa; margin-bottom: 0; }

/*返回顶部*/
.gototop { position: fixed; z-index: 99999; width: 40px; height: 40px; overflow: hidden; background: rgba(0,0,0,0.5); right: 15px; bottom: 120px; border-radius: 20px; font-size: 16px; line-height: 40px; text-align: center; }
.gototop a { display: block; color: #fff; }

