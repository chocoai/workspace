.spinner-sprite,.spinner .decrease,.spinner .decrease[disabled],.spinner .increase,.spinner .value,.spinner .value.passive { background: url('../images/spinner.png') no-repeat }
.spinner { height: 32px; width: 122px; overflow: hidden; margin: 8px 0; }
.spinner button,
.spinner .value { text-align: center; display: block; float: left; height: 100%; line-height: 32px; margin: 0 }
.spinner button { border: none; width: 35px; color: #e5312a; font: 22px Arial bold; padding: 0; outline: none }
.spinner .decrease { background-position: 0 -64px; cursor: pointer; text-indent: -10000px }
.spinner .decrease[disabled] { background-position: 0 -96px; cursor: default }
.spinner .increase { background-position: 0 0; cursor: pointer; text-indent: -10000px }
.spinner .value { background-position: 0 -128px; background-repeat: repeat-x; width: 52px; height: 32px; border: none; font-family: Arial; color: #333; padding: 0px; border-top: 1px solid #eaeaea; border-bottom: 1px solid #eaeaea; }
.spinner .value.passive { background-position: 0 -32px; color: #919191 }
.spinner .error,
.spinner .invalid { background: #aa0000 }
