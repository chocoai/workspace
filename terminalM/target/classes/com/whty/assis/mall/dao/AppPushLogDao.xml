<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.whty.assis.mall.dao.AppPushLogDao">

	<resultMap type="com.whty.assis.mall.model.AppPushLog" id="AppPushResult">
		<result property="id" column="id" />
		<result property="commandText" column="command_text" />
		<result property="deviceId" column="device_id" />
		<result property="userId" column="user_id" />
		<result property="classId" column="class_id" />
		<result property="createTime" column="create_time" />
		<result property="updateTime" column="update_time" />
		
		<result property="appPushId" column="app_push_id" />
		
	</resultMap>
	
	<delete id="deleteById" parameterType="int">
		delete from t_app_push_log where id=#{_parameter}
	</delete>
	
	<insert id="save" parameterType="com.whty.assis.mall.model.AppPushLog">
		insert into t_app_push_log(
			app_push_id,
			commandText,
			device_id,
			update_time,
			user_id,
			class_id,
			create_time
			) values (
				#{appPushId},
				#{commandText},
				#{deviceId},
				#{updateTime},
				#{userId},
				#{classId},
				#{createTime}
			)
	</insert>
	
	<select id="listByCondition" parameterType="map" resultMap="AppPushResult">
		select * from t_app_push_log
	</select>
	
	
</mapper>
