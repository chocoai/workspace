    </if>
    <if test="provinceCode != null">
      and d.province_code >= #{provinceCode}
    </if>
    GROUP BY a.creatTime
    ORDER by a.creatTime
  </select>

  <!--应用统计-->
  <select id="selectBySoftware" parameterType="map"  resultMap="BaseResultMap">
   SELECT COUNT(t.subject_id)userCount,t.software_name
   FROM t_data_gather_count_log  t
   INNER JOIN t_device_info d ON d.id = t.device_id
   WHERE 1=1
    <if test="startTime != null">
      and t.create_time >= #{startTime}
    </if>
    <if test="endTime != null">
      <![CDATA[and t.create_time <= #{endTime}]]>
    </if>
    <if test="schoolId != null">
      and d.school_id >= #{schoolId}
    </if>
    <if test="areaCode != null">
      and d.area_code >= #{areaCode}
    </if>
    <if test="cityCode != null">
      and d.city_code >= #{cityCode}
    </if>
    <if test="provinceCode != null">
      and d.province_code >= #{provinceCode}
    </if>
   GROUP BY t.software_name
   ORDER BY userCount desc
  </select>

