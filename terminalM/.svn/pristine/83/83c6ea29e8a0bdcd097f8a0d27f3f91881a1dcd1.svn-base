  <!--查询累计使用人数-->
  <select id="selectByUseCount" parameterType="map"  resultMap="BaseResultMap">
    SELECT COUNT(*)userCount,a.createTime create_time FROM
    (SELECT COUNT(*),DATE_FORMAT(t.create_time,'%Y-%m-%d')createTime,t.user_account,t.device_id FROM
    t_data_gather_count_log  t GROUP BY t.user_account,t.create_time ORDER BY createTime) a
    INNER JOIN t_device_info d ON d.id = a.device_id
    WHERE 1=1
    <if test="schoolId != null">
      and d.school_id >= #{schoolId}
    </if>
    <if test="areaCode != null">
      and d.area_code >= #{areaCode}
    </if>
    <if test="cityCode != null">
      and d.city_code >= #{cityCode}
    </if>
    <if test="provinceCode != null">
      and d.province_code >= #{provinceCode}
    </if>
    GROUP BY a.createTime
    ORDER by a.createTime
  </select>

  <!--设备开机数-->
  <select id="selectByDevice" parameterType="map"  resultMap="BaseResultMap">
    SELECT COUNT(*)userCount,a.creatTime create_time
    FROM (SELECT DATE_FORMAT(t.create_time,'%Y-%m-%d')creatTime ,t.device_id FROM
    t_data_gather_count_log  t GROUP BY t.device_id ,DATE_FORMAT(t.create_time,'%Y-%m-%d')) a
    INNER JOIN t_device_info d ON d.id = a.device_id
    WHERE 1=1
    <if test="schoolId != null">
      and d.school_id >= #{schoolId}
    </if>
    <if test="areaCode != null">
      and d.area_code >= #{areaCode}
    </if>
