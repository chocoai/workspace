<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.whty.assis.basicdata.dao.GradeDao">

	<resultMap type="com.whty.assis.basicdata.model.Grade" id="GradeResult">
		<id property="id" column="id" />
		<result property="name" column="name" />
	</resultMap>
	
	
	<select id="listByCondition" parameterType="map" resultMap="GradeResult">
		select * from t_base_grade
	</select>
	
	<select id="listByConditionByAppId" parameterType="int" resultMap="GradeResult">
		select t2.* from t_app_grade t1 left join t_base_grade t2 on t1.grade_id = t2.id where t1.app_id=#{_parameter}
	</select>
	
</mapper>