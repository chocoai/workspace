/*******************************************  房产首页  *****************************************/
/**   logo  **/
.fc_top { background:#fff !important;}
.top{ width:100%; padding:0px; height:160px; background-color:#fafafa; font-family:"Microsoft YaHei";}
.box{ width:1280px; margin:0 auto; font-family:"Microsoft YaHei";font-size:14px; height:auto; overflow:hidden;!important}
.top .logo{ float:left;font-size:30px; height:65px; font-family:"Microsoft YaHei"; font-weight:bold; margin:65px 0px;}
.top .search{float:left;border:2px solid #E30000;height:44px;width:560px; margin:55px 0 0 170px;}
.top .search select{display:none;}
.top .search .select_box{color:#999999;width:90px;line-height:32px;float:left;position:relative;font-family:"Microsoft YaHei"; font-size:16px;;border-right:1px solid #ccc; margin-top:6px;}
.top .search .select_showbox{height:32px;background:url(../images/search_ico.png) no-repeat 72px center;text-indent:20px; color:#222; font-size:16px;}
.top .search .select_showbox.active{background:url(../images/search_ico_hover.png) no-repeat 72px center;}
.top .search .select_option{ float:left;width:90px;line-height:30px; border:2px solid #E30000;border-top:none;display:none;left:-2px;top:38px;position:absolute;z-index:99999;background:#fff;text-align:left; margin:0px; padding:0px; z-index:9999;height:auto; overflow:hidden;!important}
.top .search .select_option li{ float:left;width:90px; height:30px; line-height:30px;cursor:pointer;margin:0px; padding:0px; text-align: left; font-size:16px; color:#222; text-indent:20px;}
.top .search .select_option li.selected{background-color:#F3F3F3;color:#999;text-align:left; text-indent:20px;}
.top .search .select_option li.hover{background:#BEBEBE;color:#fff;}
.top .search input.inp_srh,.search input.btn_srh{border:none;background:none;height:44px;line-height:44px;float:left; font-size:16px;}
.top .search input.inp_srh{outline:none; width:340px; padding:0px 10px; color:#666;}
.top .search .search_submit { width:100px; height:44px; float:right; background-color:#E30000;}
.top .search .search_submit .btn_srh{ width:100px; height:44px; background:url(../images/icon_search.png) no-repeat center center; cursor:pointer;}



