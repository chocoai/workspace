    <script type="text/javascript">
    // top显示当前年月日
    function today(){
      var str='';
      var d=new Date();
      str+=d.getFullYear()+'年';
      str+=(d.getMonth()+1)+'月';
      str+=d.getDate()+'日';
      return str;
    }
    $("#date").append(today());

    // banner轮播滚动
    $('#slide').slideBox({
      duration : 0.3,//滚动持续时间，单位：秒
      easing : 'linear',//swing,linear//滚动特效
      delay : 3,//滚动延迟时间，单位：秒
      hideClickBar : false,//不自动隐藏点选按键
      clickBarRadius : 10
    });


    // 视频tab
    var $tabli=$(".tab-list li");
    $tabli.mouseover(function(){
        $(this).addClass("active").siblings().removeClass("active");
        var index = $(".tab-list li").index(this);
        $(".usually-tabcontent .tab-cell").eq(index).show().siblings().hide();
    })

    // 电子报tab
    var $epapertabli=$(".epapertab-list li");
    $epapertabli.mouseover(function(){
        $(this).addClass("active").siblings().removeClass("active");
        var index = $(".epapertab-list li").index(this);
        $(".home-epaper .tab-cell").eq(index).show().siblings().hide();
    })

    // nav
    $(".nav-list li.active a").parent().next().find('a').css({'background':'url(../images/myicon.png) no-repeat 0 -250px'});

  </script>
