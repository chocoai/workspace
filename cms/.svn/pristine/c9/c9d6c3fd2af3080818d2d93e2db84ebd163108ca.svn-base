			<input class="inp_srh" name="keyword" value="${keyword!'请输入您要搜索的内容关键字'}" id="keyword" placeholder="请输入您要搜索的内容关键字"
				onfocus="if(this.value=='请输入您要搜索的内容关键字')this.value=''" onkeypress="qkeypress()"
				onblur="if(this.value=='')this.value='请输入您要搜索的内容关键字'" >
			<div class="search_submit">
							<input class="btn_srh" style="color: white;" onclick="searchHouse()" type="button" value="搜索">
