<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>换客--东楚网</title>
<link rel="stylesheet" type="text/css" href="/${res}/css/hkkj.css" />
<link rel="stylesheet" type="text/css" href="/${res}/css/public_all.css" />
<link rel="stylesheet" type="text/css" href="/${res}/css/global.css" />
<script type="text/javascript" src="/${res}/js/dc/settitle.js"></script>
<!--banner切换-->
<script src="/${res}/js/dc/jquery-1.8.3.min.js"></script>
<script src="/${res}/js/dc/banner.js"></script>
<script type="text/javascript">
$(function(){
$("#banSlide").banSlide({
    width: 1920,
    height: 380,
    responsiveWidth:710,
    pagination: {
        effect: "fade"
    },
    effect: {
        fade: {
            speed: 400
        }
    },
    play: {
        effect: "fade",
        interval: 3000,
        auto: true,
        pauseOnHover: true,
        restartDelay: 2500
    }
});
});
</script>
<!--banner切换-->
<!--新闻滚动-->
<script src="/${res}/js/dc/scroll.js"></script>
<script type="text/javascript">
$(function(){
	$('.myscroll').myScroll({
		speed: 40, //数值越大，速度越慢
		rowHeight: 36 //li的高度
	});
});
</script>
<!--新闻滚动-->
</head>

<body>

[#include "../include/hk_navi.html"/]

<!--top end-->

<!--nav 开始-->
<div id="nav">
   <div class="box">
      <ul class="nav_box">
        <li><a href="${base}/">首 页</a></li>
	    <li><a href="${base}/tops.jhtml">头条</a></li>
		<li><a href="${base}/jds.jhtml">焦点新闻</a></li>
		<li><a href="${base}/topic.jspx">专题</a></li>
		<li><a href="${base}/xsq/index.jhtml">县市区新闻</a></li>
		<li><a href="/bbs" target="_blank">东楚论坛</a></li>
		<li><a href="${base}/vedio/index.jhtml" target="_self">东楚视频</a></li>
		<li><a href="${base}/picture/index.jhtml">图轻松</a></li>
		<li><a href="${base}/baoliao.jspx" target="_self">投诉爆料台</a></li>
		<li><a href="${base}/gsgg/index.jhtml">公示公告</a></li>
		<li><a href="${base}/activity/index.jhtml" target="_self">活动</a></li>
		<li><a href="${base}/hk/index.jhtml" target="_self" class="current">换客</a></li>
		<li><a href="http://www.07448.com/huangshi" target="_blank">秒杀</a></li>
		<li><a href="${base}/fc/index.jhtml" target="_blank">房产</a></li>
		<li><a href="http://auto.hsdcw.com" target="_blank">汽车</a></li>
		<li><a href="${base}/life/index.jhtml" target="_blank">生活</a></li>
		<li><a href="${base}/edu/index.jhtml" target="_self">教育</a></li>
		<li><a href="${base}/cj/index.jhtml" target="_self">财经</a></li>
		<li><a href="${base}/health/index.jhtml" target="_self">健康</a></li>
		<li><a href="${base}/wh/index.jhtml" target="_self">文化</a></li>
       </ul>
   </div>   
</div>
<!--nav 开始-->

<div style=" clear:both;"></div>


<!--内容 开始-->
 <div class="banner">
         <div class="myscroll">
                <ul>
                [@cms_content_list count='20' orderBy='4' titLen='20' channelOption='1' channelId=channel.id]
                 [#list tag_list as g]
                    <li><a href="${g.url}" target="_blank">[@text_cut s=g.title! len=titLen/]</a>&nbsp;&nbsp;&nbsp;&nbsp;<span>${g.date?string('yyyy-MM-dd')}</span></li>
                 [/#list]
                 [/@cms_content_list]
                </ul>
        </div>
        <div class="focus">
    		<div id="banSlide" class="banSlide">
              <div class="banSlide_01"><img src="/${res}/images/banner_change01.jpg"/></div>
              <div class="banSlide_02"><img src="/${res}/images/banner_change02.jpg"/></div>
              <div class="banSlide_03"><img src="/${res}/images/banner_change03.jpg"/></div> 
	        </div>   
        </div>
                     
 </div>
 
<div class="box mt10">
<div class="content">
         [@cms_channel_list parentId='60']
         [#list tag_list as ch]
         <div class="floor">
               <div class="title"><span>换</span><a href="${ch.url!}">${ch.name}</a></div>
               <div class="ads_left">
                  <img src="/${res}/images/hk_${ch_index+1}.jpg" width="300" height="440" />
               </div>
               <ul class="product">
                    [@cms_content_list count="8" channelId=ch.id]
                    [#list tag_list as t]
                    [#if  t_index lte 4]
                    <li class="border_tab">
                       <a href="${t.url}" target="_blank">
                        [#if t.pictures?size >0]
                                    [#list t.pictures as p]
	                                [#if p_index = 0]
	                                  <img src="${p.imgPath!}" />
	                                [/#if]
	                                [/#list]
	                      [#else]<img src="${t.titleImg!t.typeImg!site.defImg}"/>
                          [/#if]
                       </a>
                       <div style="clear:both;"></div>
                       <h2><a href="${t.url}" target="_blank"><span>换</span>[@text_cut s=t.title! len='10' /]</a></h2>
                    </li>
                    [#else]
                    <li class="border_tab2">
                       <a href="${t.url}" target="_blank">
                        [#if t.pictures?size >0]
                                    [#list t.pictures as p]
	                                [#if p_index = 0]
	                                  <img src="${p.imgPath!}" />
	                                [/#if]
	                                [/#list]
	                      [#else]<img src="${t.titleImg!t.typeImg!site.defImg}"/>
                          [/#if]
                          
                       </a>
                       <div style=" clear:both;"></div>
                       <h2><a href="${t.url}" target="_blank"><span>换</span>[@text_cut s=t.title! len='10' /]</a></h2>
                    </li>
                    
                    [/#if]
                    [/#list]
                    [/@cms_content_list]
               </ul>
         </div>
         [/#list]
         [/@cms_channel_list]
           
    </div>           
</div>
<!--内容 end-->


<div style=" clear:both;"></div>
<!--友情链接 开始-->
<div class="friend_link mb10">
                <div id="Tab_link">
                        <div class="Menubox">
                          <ul>
                            <li id="menuc1" onmouseover="setTab('menuc',1,2)" class="hover">二手市场</li>
                            <li id="menuc2" onmouseover="setTab('menuc',2,2)">快递物流</li>
                          </ul>
                        </div>
                        
                        <div class="Contentbox">
                          <div id="con_menuc_1" class="hover">
                              <ul>   [@cms_friendlink_list ctgId="5"]
                                     [#if tag_list?size gt 0]
                                     [#list tag_list as link]
                                     <li><a href="${link.domain}" onclick="$.get('${base}/friendlink_view.jspx?id=${link.id}')" target="_blank">${link.name}</a></li>
                                     [/#list]
                                     [#else]<li>&nbsp;</li>
                                     [/#if]
                                     [/@cms_friendlink_list]
                                     
                               </ul>
                          </div>
                          <div id="con_menuc_2" style="display:none">
                               <ul> [@cms_friendlink_list ctgId="5"]
                                     [#if tag_list?size gt 0]
                                     [#list tag_list as link]
                                     <li><a href="${link.domain}" onclick="$.get('${base}/friendlink_view.jspx?id=${link.id}')" target="_blank">${link.name}</a></li>
                                     [/#list]
                                     [#else]<li>&nbsp;</li>
                                     [/#if]
                                     [/@cms_friendlink_list]
                                </ul>
                          </div>
                          
                        </div>
                        
                 </div>
                               
        </div>   
<!--友情链接 结束-->
        

<!--尾部 开始-->
[#include "../include/footer.html"/]
<!--尾部 开始-->

</body>
</html>