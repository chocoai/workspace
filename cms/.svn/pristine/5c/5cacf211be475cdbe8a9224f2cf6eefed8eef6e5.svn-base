/**   header  **/
.set ul.login{ float:left; width:600px; height:36px; line-height:36px;}
.set ul.login li{ float:left; width:auto; padding-right:30px;text-align:left;}
.set ul.login li .text{ width:100px; height:24px; line-height:24px; border:1px solid #e6e6e6; padding:0px 5px;}
.set ul.login li .sure{ float:left; width:60px; height:24px; background-color:#fdd000; color:#987d00; border:0px; margin-top:6px;}


/*******************************************  左  *****************************************/
.left{float:left; width:896px; margin-top:10px; margin-bottom:10px;}
.left .blt_box{ float:left; width:852px; ; border:2px solid #feb0b7; padding:20px; margin-bottom:10px;background:#fff;}
.left .blt_box .blt_title{ float:left; width:260px; height:20px; line-height:20px; padding:5px;}
.left .blt_box .send_mess_box { float:left; width:852px; margin-top:20px;}
.left .blt_box .send_mess_box textarea { float:left; width:842px; height:120px;border:1px solid #e6e6e6; margin-bottom:5px;}
.left .blt_box .send_mess_box span { float:left; margin-right:20px; height:30px; line-height:34px; margin-top:10px;}
.left .blt_box .send_mess_box span img { margin-top:10px;}
.left .blt_box .send_mess_box span input{ text-indent:5px;}

.left .blt_box .send_mess_box .send_butt { float:right; margin-top:10px; width:54px; height:24px; line-height:24px; text-align:center; color:#fff; background:#ff9600; border-radius:2px; overflow:hidden; font-size:12px;}
.left .blt_box .send_mess_box .send_butt a { text-decoration:none; display:block; color:#fff;}
.left .blt_box .send_mess_box .send_butt a:hover { background:#dc8100;}
#Tab_link{ margin:10px auto;width:896px;}
#Tab_link .Menubox{ float:left; width:896px;height:45px;background-color:#fff;} 
#Tab_link .Menubox ul{ float:left; width:896px;height:45px;list-style:none;padding:0;position:absolute; border-bottom:1px solid #eee;} 
#Tab_link .Menubox ul li{float:left;width:auto;padding-left:25px;padding-right:25px; height:45px;line-height:45px;display:block;cursor:pointer;text-align:center;color:#555;font-size:18px; border-right:1px solid #f6f6f6;}
#Tab_link .Menubox ul li.hover{ background-color:#f6f6f6;cursor:pointer;color:#f54343; border-bottom:1px solid #f54343;}
#Tab_link .Contentbox{ float:left;width:896px; padding-top:20px;background-color:#fff; height:auto; overflow:hidden;!important} 
#Tab_link .Contentbox ul{float:left;width:896px;height:auto; overflow:hidden;!important} 
#Tab_link .Contentbox ul li{ float:left; width:876px; border-bottom:1px solid #f6f6f6; padding-top:15px; padding-bottom:10px; margin-left:10px;height:auto; overflow:hidden;!important}
#Tab_link .Contentbox ul li .user_pic{ float:left; width:60px; padding:0px 20px; line-height:30px;}
#Tab_link .Contentbox ul li .cont_box{ float:left; width:776px;height:auto; overflow:hidden;!important}
#Tab_link .Contentbox ul li .cont_box h2{float:left; width:776px; height:24px;line-height:24px; text-align:left;}
#Tab_link .Contentbox ul li .cont_box h2 a{ font-size:18px; font-weight:bold; color:#0a0a0a;}
#Tab_link .Contentbox ul li .cont_box h2 a:hover{ text-decoration:none;}
#Tab_link .Contentbox ul li .cont_box .bl_cont{float:left; width:776px;height:auto; overflow:hidden;!important }
#Tab_link .Contentbox ul li .cont_box .bl_cont a{ font-size:14px;color:#666;}
#Tab_link .Contentbox ul li .cont_box .bl_cont a:hover{ text-decoration:none;}
#Tab_link .Contentbox ul li .cont_box .pic{ float:left; width:776px; margin-top:5px;}
#Tab_link .Contentbox ul li .cont_box .pic img{ float:left; width:100px; height:90px; overflow:hidden; margin-right:5px;}
#Tab_link .Contentbox ul li .cont_box .time{ float:left; width:776px; height:26px; line-height:26px;color:#999; margin-top:10px; text-indent:20px; background:url(../images/time.png) no-repeat left center;}
#Tab_link .Contentbox ul li .cont_box .time a{ font-size:14px;color:#999; margin-right:10px;}
#Tab_link .Contentbox ul li .cont_box .time a:hover{ text-decoration:none;}
#Tab_link .Contentbox ul li .cont_box .time span{ float:right;}
#Tab_link .Contentbox ul li .cont_box .time span a{ font-size:14px;color:#999; margin-left:10px;}


/*******************************************  右  *****************************************/
.right{float: right; width:374px; margin-top:10px; height:auto; overflow:hidden;!important}
.right .need{ float:left; padding:0px 10px 10px 10px;width:354px;background-color:#fff; height:auto; overflow:hidden;!important}
.right .need .title{ margin:0 auto;width:354px; height:40px; border-bottom:1px solid #f3f3f3; background:none;}
.right .need .title span{ float:left; width:auto; padding-left:6px; padding-right:6px; height:39px; line-height:45px; border-bottom:2px solid #cc0303; font-size:18px; color:#272727; font-weight:bold;}
.right .need .hot_article{ float:left; width:354px; margin:10px 0px;height:auto; overflow:hidden;!important}
.right .need .hot_article img{ float:left; width:130px; height:90px; margin-right:10px; }
.right .need .hot_article .hot_cont{widows:200px; height:90px; font-size:12px; line-height:24px;text-align:Justify; text-justify:inter-ideograph;}
.right .need .hot_article .hot_cont a{font-size:12px;}
.right .need .hot_article ul{ margin:5px auto;width:350px;height:auto; overflow:hidden;!important}
.right .need .hot_article ul li{float:left;width:350px;height:26px;line-height:26px;background: url(../images/dics.png) no-repeat left center;text-indent:10px;}
.right .need .hot_article ul li span{ float:right; color:#b8b6b6;}

/*******************************************  详情页  *****************************************/
/****  map   ****/
.map{ margin:0 auto; width:1280px; height:36px; background-color:#fff; line-height:36px; font-size:14px; text-align:left;}
.map img{ float:left; width:20px; height:18px; margin:10px;}
.map a{ text-decoration:none; margin:0px 5px;}
.map a:hover{ text-decoration: underline;}

.details{ float:left; width:856px;background-color:#fff; margin:10px 0px; padding:20px 20px 0px 20px; height:auto; overflow:hidden;!important}
.details .article_title{ float:left; width:856px; padding:0px 20px; line-height:30px; font-size:20px; color:#333; text-align:center; font-weight:bold;height:auto; overflow:hidden;!important }
.details .time{ float:left; width:856px; padding:5px 10px; line-height:30px; font-size:12px; color:#333; text-align:center;}
.details .time a{ margin-right:20px; font-size:12px; }
.details .article{ margin:20px auto 10px; width:856px;text-align:Justify;text-justify:inter-ideograph; padding-top:20px; padding-bottom:20px; border-top:1px solid #eee;height:auto; overflow:hidden;!important}
.details .article p{text-indent:2em; margin-bottom:10px;}
.details .article a:link{ color:#999; text-decoration:none; font-size:13px;}
.details .article a:hover{ color:#1d9dba; text-decoration: underline;}
.details .article .progress{ float:left; width:896px;margin-top:50px;border-top:1px solid #eee;height:auto; overflow:hidden;!important}
.details .article .progress h6{ font-size:14px; font-weight:bold; height:40px; line-height:40px; margin-top:10px;}
.details .article .progress .progress_cont{ float:left; width:814px; background-color:#ffffcc; border:1px solid #cc0000; padding:20px;height:auto; overflow:hidden;!important}
.progress_cont h1,.progress_cont h2,.progress_cont h3{ font-weight:normal; line-height:24px;}
.progress_cont h2{ margin:10px 0px;}

.details .send_mess{ float:left; width:856px; font-size:14px; font-weight:bold; height:30px; line-height:30px; border-bottom:1px solid #e0e0e0;}
.details .send_mess_box { float:left; width:856px; margin:10px 20px 20px 0px;}
.details .send_mess_box textarea { float:left; width:834px; height:100px; padding:10px;border:1px solid #e6e6e6;}
.details .send_mess_box span { float:left; margin-top:10px; margin-right:10px;}
.details .send_mess_box .send_butt { float:right; margin-top:10px; width:54px; height:24px; line-height:24px; text-align:center; color:#fff; background:#ff9600; border-radius:2px; overflow:hidden; font-size:12px;}
.details .send_mess_box .send_butt a { text-decoration:none; display:block; color:#fff;}
.details .send_mess_box .send_butt a:hover { background:#dc8100;}
.details .review_all{ float:left; width:856px;}
.details .review_all .sort{ float:right; text-align:left;height:30px; line-height:30px; margin:10px 0px 0px;}
.details .review_all .sort u{ float:left; text-decoration:none; width:auto; padding:0px 5px;}
.details .review_all .sort span{ float:left; width:60px; height:28px; line-height:28px; text-align:center; border:1px solid #f6f6f6;}
.details .review_all .sort span:hover{ background-color:#ea606c; color:#fff;}
.details .review_all .sort span.now{ background-color:#ea606c; color:#fff;}
.details .review_all ul{ float:left;}
.details .review_all ul li{ float:left; width:856px; padding:20px 0px 0px 0px; border-bottom:1px solid #e6e6e6;}
.details .review_all_l{ float:left; width:60px; height:60px; overflow:hidden; border-radius:50%;}
.details .review_all_r{ float:left; width:780px; margin-left:15px;}
.details .reviewer{ float:left; width:780px; height:24px; line-height:24px; margin:5px 0;}
.details .reviewer span{ float:left; width: auto; height:24px; margin-right:30px;}
.details .reviewer span a{ text-decoration: none; font-weight:bold;}
.details .reviewer span a:hover{ text-decoration: underline;}
.details .reviewer u{ float:left; width: auto; height:24px; margin-right:30px; text-decoration:none;}
.details .reviewer i{ float: right; width: auto; height:24px; margin-left:20px; font-style:normal;}
.details .review_cont{float:left; width:760px;line-height:24px;font-style:normal;text-align:justify;padding:10px;background-color:#f6f6f6;margin:5px 0px; height:auto; overflow:hidden;!important}
.details ul.reply{ float:left; width:780px; height:30px; line-height:30px;}
.details ul.reply li{ float:left; width: auto;line-height:30px; text-align:left; text-indent:20px; padding-top:0px;border:0px; margin-right:50px;}
.details ul.reply li.zan{ text-decoration:none; color:#666; background: url(../images/zan.png) no-repeat left center;}
.details ul.reply li.zan:hover{ text-decoration:none; color:#000; background: url(../images/zan1.png) no-repeat left center; cursor:pointer;}
.details ul.reply li.support{ text-decoration:none; color:#000; background: url(../images/zan1.png) no-repeat left center; cursor:pointer;}


.details ul.reply li.reversion{ text-decoration:none; color:#666; background: url(../images/reply.png) no-repeat left center;}
.details ul.reply li.reversion:hover{ text-decoration:none; color:#000; background: url(../images/reply1.png) no-repeat left center;cursor:pointer;}
.details ul.reply li.search_reply{ text-decoration:none; color:#666; background: url(../images/search_reply.png) no-repeat left center;}
.details ul.reply li.search_reply:hover{ text-decoration:none; color:#000; background: url(../images/search_reply1.png) no-repeat left center;cursor:pointer;}
.details .send_mess_box2 { float:left; width:780px; margin:0px 20px 10px 0px;}
.details .send_mess_box2 textarea { float:left; width:758px; height:100px; padding:10px;border:1px solid #e6e6e6;}
.details .send_mess_box2 span { float:left; margin-top:10px; margin-right:10px;}
.details .send_mess_box2 .send_butt { float:right; margin-top:10px; width:54px; height:24px; line-height:24px; text-align:center; color:#fff; background:#ff9600; border-radius:2px; overflow:hidden; font-size:12px;}
.details .send_mess_box2 .send_butt a { text-decoration:none; display:block; color:#fff;}
.details .send_mess_box2 .send_butt a:hover { background:#dc8100;}



.theme-popover-mask{z-index: 9998;position:fixed;top:0;left:0;width:100%;height:100%;background:#000;opacity:0.4;filter:alpha(opacity=40);display:none}
.theme-popover{z-index:9999;position:fixed;top:50%;left:50%;width:700px;min-height:360px;margin:-180px 0 0 -330px;border-radius:5px;border:solid 2px #666;background-color:#fff;display:none;box-shadow: 0 0 10px #666;}
.theme-poptit{border-bottom:1px solid #ddd;padding:12px;position: relative;}
.theme-popbod{color:#444;height:330px;overflow:auto;}
.theme-popbom{padding:15px;background-color:#f6f6f6;border-top:1px solid #ddd;border-radius:0 0 5px 5px;color:#666}
.theme-popbom a{margin-left:8px}
.theme-poptit .close{float:right;color:#999;padding:5px;margin:-2px -5px -5px;font:bold 14px/14px simsun;text-shadow:0 1px 0 #ddd}
.theme-poptit .close:hover{color:#444;}
.dform{padding:10px 30px;text-align: center;}
