.jkzj .jkzj_bd .focus .btn a{position:relative;display:inline;width:13px;height:13px;border-radius:7px;margin:0 5px;color:#B0B0B0;font:12px/15px "\5B8B\4F53";text-decoration:none;text-align:center;outline:0;float:left;background:#D9D9D9;}  
.jkzj .jkzj_bd .focus .btn a:hover,.focus .btn a.current{cursor:pointer;background:#fc114a;}  
.jkzj .jkzj_bd .focus .fPic{position:absolute;left:0px;top:0px;}  
.jkzj .jkzj_bd .focus .D1fBt{overflow:hidden;zoom:1;height:16px;z-index:10;}  
.jkzj .jkzj_bd .focus .shadow{width:100%;position:absolute;bottom:0;left:0px;z-index:10;height:40px;line-height:40px;background:rgba(0,0,0,0.6);  
filter:progid:DXImageTransform.Microsoft.gradient( GradientType = 0,startColorstr = '#80000000',endColorstr = '#80000000')\9;display:block;text-align:left;}  
.jkzj .jkzj_bd .focus .shadow a{text-decoration:none;color:#fff;font-size:14px;overflow:hidden;margin-left:10px;font-family:"\5FAE\8F6F\96C5\9ED1";}  
.jkzj .jkzj_bd .focus .fcon{position:relative;width:100%;float:left;display:none;background:#000  }  
.jkzj .jkzj_bd .focus .fcon img{display:block;}  
.jkzj .jkzj_bd .focus .fbg{bottom:-30px;position:absolute;text-align:center;z-index:200; width:352px; height:30px; background:#f5f5f5;}  
.jkzj .jkzj_bd .focus .fbg div{margin:4px auto 0;overflow:hidden;zoom:1;height:14px}    
.jkzj .jkzj_bd .focus .fbg .D1fBt { margin-left:90px; margin-top:10px;}
.jkzj .jkzj_bd .focus .D1fBt a{position:relative;display:inline;width:24px;height:9px;margin:0 10px;color:#B0B0B0;font:12px/15px "\5B8B\4F53";text-decoration:none;text-align:center;outline:0;float:left;background:#D9D9D9;}    
.jkzj .jkzj_bd .focus .D1fBt .current,.focus .D1fBt a:hover{background:#ff9700;}    
