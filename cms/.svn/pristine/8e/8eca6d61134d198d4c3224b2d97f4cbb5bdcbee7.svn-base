/**   nav  **/
.nav{ width:100%; border:0px; height:36px; background:#cc0303;}
.nav .nav_box{ float:left; width:800px; height:36px; line-height:36px;}
.nav .nav_box li{ float:left; width:auto; height:36px; line-height:36px;text-align: center; }
.nav .nav_box li a:link,.nav .nav_box li a:visited{ color:#fff; font-size:16px; font-weight:bold; padding:0 20px; line-height:36px; text-decoration:none; display:block; background-color:#cc0303;}
