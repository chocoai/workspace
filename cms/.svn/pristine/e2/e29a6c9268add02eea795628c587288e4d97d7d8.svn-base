<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.yhcrt.weihu.cms.entity.main">
	<class name="TopicChannel" table="jc_topic_channel">
		<meta attribute="sync-DAO">false</meta>
		<id name="channelId" type="java.lang.Integer" column="channel_id"><generator class="identity"/></id>
		<property name="channelName" column="channel_name" type="string" not-null="true" length="150"/>
		<property name="channelPath" column="channel_path" type="string" not-null="false" length="150"/>
		<property name="priority" column="priority" type="integer" not-null="true" length="10"/>
		<many-to-one name="topic" column="topic_id" class="CmsTopic" not-null="true"></many-to-one>
	</class>
</hibernate-mapping>