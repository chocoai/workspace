	<script type="text/javascript">
	  function  submitForm(){
		  [#if user??]
			if ($('#cont').val() == '') {
				alert('请先填写留言内容');
				$('#cont').focus();
				return;
			}
			$('#text').val($('#cont').val());
			var s = $("#submitForm");
			var url = s[0].action;
			var data = s.serialize();
			var options = {
	                    url: url,
	                    type: 'post',
	                    dataType: 'json',
	                    data: data,
	                    success:  function (data) {
		                	if(data.status==0) {
			                    alert("提交成功");
			                    location.href=location.href;
			                }
	                    }
	                };
	                $.ajax(options);
	                return false;
	       [#else]window.location.href="${base}/login.jspx?returnUrl=${content.url}";
	       [/#if]
         }

	  function  change(){
		  [#if user??]
		    [#if content.user?? && user == content.user]alert("不能交换自己发布的物品");
		    [#else]window.location.href="${base}/member/exchange_add.jspx?contentId=${content.id}";
		    [/#if]
		  [#else]window.location.href="${base}/login.jspx?returnUrl=${location}";
		  [/#if]
	  }
      </script> 
