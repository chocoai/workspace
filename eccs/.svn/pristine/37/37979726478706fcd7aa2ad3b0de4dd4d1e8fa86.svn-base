@charset "utf-8";
*{margin:0;padding:0;border:0;}
body{ background-color:#eef5ff; color: #222;font-size: 12px; font-family: "Microsoft YaHei"; }
a{ color:#0071b1; text-decoration:none;}
a:hover{ color:#0071b1; text-decoration: underline;}


/********* jixiao **********/
#jixiao{float:left; width:100%; min-height:500px; border-top:1px solid #fff;}

#main #jixiao table.input_table{ width:1200px; margin:20px auto; text-align: center; background-color:#c2cdd3;}
#main #jixiao table.input_table tr{ height:30px; line-height:30px;}
#main #jixiao table.input_table tr td{ background-color:#efefef; text-align:left; padding-left:8px; padding-right:8px; width:84px}
#main #jixiao table.input_table tr td input{ border:1px solid #c1e9ff;line-height:22px;}

.sub{ float:right; width:80px; height:30px; text-align:center; line-height:30px; color:#fff; background-color:#01749e; font-family:"新宋体";}



