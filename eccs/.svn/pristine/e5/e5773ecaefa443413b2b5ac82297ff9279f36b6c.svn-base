public class Step3WorkerTemplate implements java.io.Serializable {

	// Fields

	private static final long serialVersionUID = 1L;

	private ProceStepDef proceStepDef;// 流程步骤

	private Step3Worker step3Worker;// 分配用户

	public Step3WorkerTemplate() {

	}

	public Step3WorkerTemplate(ProceStepDef proceStepDef,
			Step3Worker step3Worker) {
		this.proceStepDef = proceStepDef;
		this.step3Worker = step3Worker;
	}

	public ProceStepDef getProceStepDef() {
		return proceStepDef;
	}

	public void setProceStepDef(ProceStepDef proceStepDef) {
		this.proceStepDef = proceStepDef;
	}

	public Step3Worker getStep3Worker() {
		return step3Worker;
	}

	public void setStep3Worker(Step3Worker step3Worker) {
		this.step3Worker = step3Worker;
	}

}