
pre.prettyprint {
	border: 0;
	border-left: 3px solid rgb(204, 204, 204);
	margin-left: 2em;
	padding: 0.5em;
	font-size: 12px;
	display: block;
	font-family: "Consolas", "Monaco", "Bitstream Vera Sans Mono", "Courier New", Courier, monospace;
	
	
	
	line-height: 16px;
	text-indent: 0px;
	margin: 1em auto; 
	padding: 1em; 
	white-space: pre-wrap;/* 自动换行 */
}