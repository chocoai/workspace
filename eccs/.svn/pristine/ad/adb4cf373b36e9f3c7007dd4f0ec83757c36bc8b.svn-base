.yfd{float:right;}		/* 右浮动 */
.clear{clear:both;}		/* div换行 */
.zdhh{overflow:hidden;} /* 文字自动换行 */
.show{display:block;}	/* 显示 */
.hide{display:none;}	/* 隐藏 */
