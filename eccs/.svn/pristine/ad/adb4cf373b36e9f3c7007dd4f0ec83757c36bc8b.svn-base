/** ================================ 字体样式 ================================== **/
.zt1{font-family:'Arial';}
.zt2{font-family:'Microsoft YaHei','微软雅黑';}
.zt3{font-family:"黑体";}
.ztzc{font-weight:normal;}			/* 字体正常 */
.ztc{font-weight:bold;}				/* 粗体 */
.ztx{font-style:italic;}			/* 斜体 */
.zjc{text-align:center;}			/* 文字居中 */
.zjy{text-align:right;}				/* 文字居右 */
.zjz{text-align:left;}				/* 文字居左 */
.xhx{text-decoration:underline;}	/* 下划线 */
.shx{text-decoration:overline;}		/* 上划线 */
.scx{text-decoration:line-through;} /* 删除线 */
.zmdx{text-transform:uppercase;}	/* 所有字母大写 */
.zmxx{text-transform:lowercase;}	/* 所有字母小写 */
.zmkt{text-transform:capitalize;}	/* 单词首字母大写 */

/** ===================================== 其他  ================================= **/
