/* 选框 */
.newcheck{z-index: 999999;cursor: pointer;position: absolute;left: 47%;bottom:20px}
.newcheck.right{left: 48%;}
.newcheck .input_check {
    position: absolute;
    visibility: hidden;
  }
  .newcheck .input_check + .spanlabel {
    display: inline-block;
    width: 23px;
    height: 23px;
    border: 1px solid rgba(0, 0, 0, 0.3);
    border-radius:3px; 
    /* box-shadow: 0 0 1px; */
    -webkit-transition: all 0.5s ease;
    -moz-transition: all 0.5s ease;
    -o-transition: all 0.5s ease;
    -ms-transition: all 0.5s ease;
    transition: all 0.5s ease;
  }
  .newcheck .input_check:checked + .spanlabel {
    position: relative;
    background: #fff;
    /* border: 1px solid #aaa; */
  }
  .newcheck .input_check:checked + .spanlabel::after {
    content: "";
    position: absolute;
    left: 3px;
    bottom: 9px;
    width: 13px;
    height: 7px;
    border: 2px solid #309aeb;
    border-top-color: transparent;
    border-right-color: transparent;
    -ms-transform: rotate(-54deg);
    -moz-transform: rotate(-54deg);
    -webkit-transform: rotate(-54deg);
    transform: rotate(-54deg);
  }
  