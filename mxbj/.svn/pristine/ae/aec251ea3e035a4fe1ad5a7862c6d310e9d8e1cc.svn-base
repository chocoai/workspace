@font-face {  font-family: 'icofont';  src:  url('../fonts/icofont.eot?fko7cl');  src:  url('../fonts/icofont.eot?fko7cl#iefix') format('embedded-opentype'), url('../fonts/icofont.ttf?fko7cl') format('truetype'), url('../fonts/icofont.woff?fko7cl') format('woff'), url('../fonts/icofont.svg?fko7cl#icofont') format('svg'); font-weight: normal;  font-style: normal;  }

