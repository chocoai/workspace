@charset "UTF-8";

/*公共样式*/
body{color:#222;font-size:12px;line-height:1.8em;font-family:微软雅黑,Tahoma,Helvetica; /* min-width:1200px; */}
body,form,ul,ol,li,p,h1,h2,h3,h4,h5,h6,dl,dt,dd,table,fieldset,div,tr,td{margin:0; padding:0;}
